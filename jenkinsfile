pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Hello World! Build-Jenkins'
            }
        }
    stage('Test') {
            steps {
                echo 'Hello World! Test-Jenkins'
            }
        }
    stage('Deploy') {
            steps {
                echo 'Hello World! Deploy-Jenkins'
            }
        }
    }
}
